FoamFile
{
    format      ascii;
    class       volScalarField;
    object      omega;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

omegaInlet      10;

dimensions      [0 0 -1 0 0 0 0];

internalField   uniform 0.0;

boundaryField
{
    freestream
    {
        type            inletOutlet;
        inletValue      uniform 0.0;
        value           uniform 0.0;
    }

    wall
    {
        type            omegaWallFunction;
        value           uniform 0.0;
    }
    inlet
    {
	type		calculated;
	value		uniform 0.0;
    }
    outlet
    {
	type		calculated;
	value		uniform 0.0;
    }
    #includeEtc "caseDicts/setConstraintTypes"
}

