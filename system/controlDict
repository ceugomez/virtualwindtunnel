FoamFile
{
    format      ascii;
    class       dictionary;
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     simpleFoam;

solver          simpleFoam;

startFrom       latestTime;

startTime       0;

stopAt          endTime;

endTime         1E-4;

deltaT          0.1;

writeControl    adjustableRunTime;

writeInterval   1E-4;

purgeWrite      0;

writeFormat     ascii;

writePrecision   8;

writeCompression off;

timeFormat      general;

timePrecision   6;

runTimeModifiable true;

